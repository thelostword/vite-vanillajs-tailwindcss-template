    "@release-it/conventional-changelog": "^5.1.0",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.2.5",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-html": "^7.1.0",
